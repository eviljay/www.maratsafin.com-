function validate() {
  form      = document.LoginForm;
  at        = form.formEmail.value.indexOf('@');
  dot       = form.formEmail.value.indexOf('.');
  email     = form.formEmail.value;
  password  = form.formPassword.value;
  submitMSG = '';
  submitOK  = 'True';

  if( ( at == -1 || dot == -1 ) && email != '' ) {
    submitMSG += 'The specified email address is invalid.' + "\n";
    submitOK = 'False';
  }

  if( !email ) {
    submitMSG += 'Please enter your email address.' + "\n";
    submitOK = 'False';
  }

  if( !password ) {
    submitMSG += 'Please enter your password.' + "\n";
    submitOK = 'False';
  }

  if( submitOK == 'False' ) {
    alert( 'Login failed:' + "\n\n" + submitMSG );
    return false;
  }
}

